{
	"name": "node-cache-plus",
	"version": "1.0.0",
	"type": "module",
	"description": "A wrapper around node-cache with additional features such tag based invalidation, factory functions, and more.",
	"main": "dist/index.js",
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"build:docs": "typedoc --out docs ./src/index.ts",
		"check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"check": "biome check ./src --diagnostic-level=error --fix",
		"check-tsc": "tsc --noEmit",
		"test": "vitest run",
		"ci": "pnpm check && pnpm test && pnpm build && pnpm check-exports"
	},
	"keywords": [
		"cache",
		"caching",
		"local",
		"variable",
		"multi",
		"memory",
		"internal",
		"node",
		"memcached",
		"object",
		"tags",
		"factory",
		"wrapper",
		"helpers"
	],
	"tags": [
		"cache",
		"caching",
		"local",
		"variable",
		"multi",
		"memory",
		"internal",
		"node",
		"memcached",
		"object",
		"tags",
		"factory",
		"wrapper",
		"helpers"
	],
	"author": "Akshay Petta <hello@akshaypetta.com> (https://akshaypetta.com)",
	"license": "MIT",
	"devDependencies": {
		"@arethetypeswrong/cli": "~0.17.1",
		"@biomejs/biome": "1.9.4",
		"@types/node": "~22.10.2",
		"tsup": "~8.3.5",
		"typedoc": "~0.27.5",
		"typescript": "~5.7.2",
		"vitest": "~2.1.8"
	},
	"dependencies": {
		"@cacheable/node-cache": "1.5.1"
	}
}
